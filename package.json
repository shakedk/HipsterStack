{
  "name": "<%= slug %>",
  "author": "Moritz Stefaner",
  "version": "0.1.0",
  "description": "<%= description %>",
  "repository": "",
  "staging_server_URL": "http://clients.stefaner.eu",
  "staging_server_path": "starling@starling.columba.uberspace.de:/var/www/virtual/starling/html",
  "staging_server_directory": "<%= slug %>",
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel -p 8080 ./src/index.html",
    "nvm": "~/.nvm/nvm-exec which node | xargs -J % ln -sf % ./.nvm-node && chmod +x ./.nvm-node",
    "build": "rm -rf build; parcel build ./src/index.html --out-dir build --public-url ./; open build",
    "deploy-preview": "rm -rf build; npm run build; cp -R build $npm_package_version; scp -r $npm_package_version $npm_package_staging_server_path/$npm_package_staging_server_directory/; rm -rf $npm_package_version; echo Uploaded to $npm_package_staging_server_URL/$npm_package_staging_server_directory/$npm_package_version; echo $npm_package_staging_server_URL/$npm_package_staging_server_directory/$npm_package_version | pbcopy"
  },
  "devDependencies": {
    "babel-core": "^6.26",
    "babel-plugin-transform-class-properties": "^6.24",
    "babel-plugin-transform-decorators": "^6.24",
    "babel-plugin-transform-decorators-legacy": "^1.3",
    "babel-preset-env": "^1.7",
    "babel-preset-react": "^6.24",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass": "^4.9.4",
    "parcel-bundler": "^1.10",
    "babel-eslint": "^10.0.1"
  },
  "dependencies": {
    "d3": "^5.7",
    "lodash": "^4.17",
    "mobx": "^5.5",
    "mobx-react": "^5.3",
    "mobx-utils": "^5.0",
    "react": "^16.5",
    "react-dom": "^16.5",
    "react-hot-loader": "^4.3"
  }
}
